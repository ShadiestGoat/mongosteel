{
  "name": "mongosteel",
  "version": "1.3.2",
  "description": "A solid ODM for MongoDB",
  "main": "dist/cjs/index",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index",
  "author": "Shady Goat <shady@shadygoat.eu> (https://shadygoat.eu/)",
  "license": "MIT",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "lint": "eslint 'lib/**/*.ts'",
    "build:cjs": "tsc -m commonjs --outDir dist/cjs",
    "build:esm": "tsc",
    "clean": "rm -rf dist docs/dist",
    "build:docs": "typedoc",
    "minify": "node minify.js",
    "build:full": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:docs && npm run minify",
    "prepublishOnly": "npm run build:full"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ShadiestGoat/mongosteel.git"
  },
  "bugs": {
    "url": "https://github.com/ShadiestGoat/mongosteel/issues"
  },
  "homepage": "https://github.com/ShadiestGoat/mongosteel",
  "keywords": [
    "mongodb",
    "orm",
    "odm",
    "schema",
    "database",
    "db",
    "nosql",
    "data",
    "mongosteel"
  ],
  "dependencies": {
    "mongodb": "~4.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.31.0",
    "terser": "^5.8.0",
    "typedoc": "^0.21.8",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
